<!DOCTYPE html>
<html>
<head>
    <title>Show Users</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
</head>
<body>
<h1>User List</h1>

<table>
    <thead>
        <tr>

            <th>First Name</th>
            <th>Last Name</th>
            <th>Email Address</th>
        </tr>
    </thead>
    <tbody>
    <% users.forEach(function(user, index){ -%>
        <tr>
            <td><%= user.firstName %></td>
            <td><%= user.lastName %></td>
            <td><%= user.email %></td>
        </tr>
    <% }); %>
    </tbody>
</table>


</body>
</html>